{
  "name": "CartographicLineSymbol",
  "type": "class",
  "baseClass": "LineSymbol",
  "baseLocation": "symbols/LineSymbol.json",
  "variable": "line",
  "description": "Line symbols are used to draw linear features on the graphics layer. CartographicLineSymbol, like SimpleLineSymbol, is either a solid line or a predefined pattern of dashes and dots. Line joins and line caps can be added to further define the line symbol.",
  "summary": "draw polylines and outline of points and polygons",
  "path": "esri/symbols/CartographicLineSymbol",
  "toolbar": true,
  "constructorArgs": ["style","color","width","cap","join","miterLimit"],
  "properties": [
    {
      "name": "style",
      "type": "String",
      "default": "STYLE_SOLID",
      "method": "setStyle",
      "summary": "The line style",
      "options": {
        "type": "constants",
        "className": "CartographicLineSymbol",
        "values": [
          "STYLE_DASH",
          "STYLE_DASHDOT",
          "STYLE_DASHDOTDOT",
          "STYLE_DOT",
          "STYLE_LONGDASH",
          "STYLE_LONGDASHDOT",
          "STYLE_NULL",
          "STYLE_SHORTDASH",
          "STYLE_SHORTDASHDOT",
          "STYLE_SHORTDASHDOTDOT",
          "STYLE_SHORTDOT",
          "STYLE_SOLID"
        ]
      }
    },
    {
      "name": "cap",
      "type": "String",
      "default": "CAP_BUTT",
      "method": "setCap",
      "summary": "The cap style",
      "options": {
        "type": "constants",
        "className": "CartographicLineSymbol",
        "values": [
          "CAP_BUTT",
          "CAP_ROUND",
          "CAP_SQUARE"
        ]
      }
    },
    {
      "name": "join",
      "type": "String",
      "default": "JOIN_MITER",
      "method": "setJoin",
      "summary": "The join style",
      "options": {
        "type": "constants",
        "className": "CartographicLineSymbol",
        "values": [
          "JOIN_BEVEL",
          "JOIN_MITER",
          "JOIN_ROUND"
        ]
      }
    },
    {
      "name": "miterLimit",
      "type": "Number",
      "default": "10",
      "method": "setMiterLimit",
      "summary": "Size threshold for showing mitered line joins"
    }
  ],
  "methods": [
    {
      "name": "setCap",
      "parameters": [
        {
          "name": "cap",
          "type": "property"
        }
      ],
      "summary": "Sets the cap style"
    },
    {
      "name": "setJoin",
      "parameters": [
        {
          "name": "join",
          "type": "property"
        }
      ],
      "summary": "Sets the join style"
    },
    {
      "name": "setMiterLimit",
      "parameters": [
        {
          "name": "miterLimit",
          "type": "property"
        }
      ],
      "summary": "Sets the size threshold for showing mitered line joins"
    },
    {
      "name": "setStyle",
      "parameters": [
        {
          "name": "style",
          "type": "property"
        }
      ],
      "summary": "Sets the line symbol style"
    }
  ]
}